// testing whether transactions available only in certain states are generated properly
main contract A {

   A() {
       ->S1;
   }

   state S1 {}
   state S2 {};
   state S3 {
       int y;
   }


   transaction t(A@(S1|S2) this) {
       int x = 3;
   }

   transaction inState3(A@S3 this) {
       y = 4;
   }

}