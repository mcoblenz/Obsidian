// testing whether transactions available only in certain states are generated properly
main contract A {

   A() {
       ->S1;
   }

   state S1 {}
   state S2 {};
   state S3 {
       int y;
   }


   transaction t() available in S1, S2 {
       int x = 3;
   }

   transaction inState3() available in S3 {
       y = 4;
   }

}