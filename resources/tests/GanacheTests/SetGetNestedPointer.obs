contract IntContainer{
    int x;

    IntContainer@Owned(int init) {
        x = init;
    }

    transaction set(int value) {
        x = value;
    }

    transaction get() returns int{
        return x;
    }
}

contract IntContainerWrapper{
    IntContainer@Owned ic;

    IntContainerWrapper@Owned() {
        ic = new IntContainer(0);
    }

    transaction set(int value) {
        ic.set(value);
    }

    transaction get() returns int{
        return (ic.get());
    }
}

