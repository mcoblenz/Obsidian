main contract PrimOps{
        transaction primops() {
           int x;
           bool y;

           y = true && false;
           y = true || false;

           x = 4 + 4;
           x = 4 - 4;
           x = 4 / 4;
           x = 4 * 4;
           x = 4 % 4;

           y = 1 == 2;
           y = 1 != 2;
           y = 3 > 4;
           y = 3 >= 4;
           y = 3 < 3;
           y = 3 <= 3;
           y = 3 != 3;

           y = !y;
           x = - x;

           return;
        }
}
