contract IntLLEnd{
    int payload;

    IntLLEnd@Owned(int p){
        payload = p;
    }

    transaction sum() returns int {
        return payload;
    }
}

contract IntLL1{
    int payload;
    IntLLEnd@Unowned next;

    IntLL1@Owned(int p, IntLLEnd n){
        payload = p;
        next = n;
    }
}

contract IntLL2{
    int payload;
    IntLL1@Unowned next;

    IntLL2@Owned(int p, IntLL1 n){
        payload = p;
        next = n;
    }
}

contract IntLL3{
    int payload;
    IntLL2@Unowned next;

    IntLL3@Owned(int p, IntLL2 n){
        payload = p;
        next = n;
    }
}

main contract LinkedListShort {
    IntLLEnd@Owned list;

    transaction LinkedListShort() {
    }

    transaction main() returns int {
        list = new IntLLEnd(57005);
        return list.sum();
    }

    transaction sum() returns int {
        return list.sum();
    }
}
